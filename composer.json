{
    "name": "dot-im/control-panel",
    "description": "Simple laravel control panel",
    "keywords": ["laravel", "control panel", "admin panel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ammar Ahmad Kannas",
            "email": "ammarkannas@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": ">=5.5"
    },
    "autoload": {
        "psr-4": {
            "Dotim\\CP\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dotim\\CP\\Providers\\ControlPanelServiceProvider"
            ],
            "aliases": {
                "ControlPanel": "Dotim\\CP\\Facades\\ControlPanel"
            }
        }
    }
}
